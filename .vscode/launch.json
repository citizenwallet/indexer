  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Node",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/node/main.go",
      "args": ["-env", "../../.env", "-rate", "99", "-sync", "1"]
    },
    {
      "name": "Launch API",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/node/main.go",
      "args": ["-env", "../../.env", "-rate", "99", "-sync", "1", "-onlyApi"]
    }
  ]
}
